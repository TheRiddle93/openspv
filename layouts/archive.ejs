<!DOCTYPE html>
<html lang="en">
<head>
<title>Archive | Ryan X. Charles</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link href="/bootstrap/css/bootstrap.min.css" rel="stylesheet">
<link href="/css/style.css" rel="stylesheet" media="screen">
</head>
<body class="page-archive">
<div class="container">
  <div class="row-fluid">
    <div class="span4">
      <div class="headerbox">
        <a href="/"><img src="/images/andromeda-circle.png" alt="Ryan X. Charles"></a>
        <h1>Ryan X. Charles</h1>
        <p>Anti-aggression. Pro-bitcoin.</p>
        <p><a href="/">Home</a> | Archive | <a href="/about/">About</a></p>
      </div>
    </div>
    <div class="span8">
      <% for (var i=0; i<documents.length; i++) { %>
        <article>
          <h1><a href="/archive/<%- documents[i].filename %>/"><%- documents[i].header.title %></a></h1>
          <%
            var d=documents[i].header.date;
            var dyear=d.getFullYear();
            var dmonth=d.getMonth()+1;
            if (String(dmonth).length==1) {dmonth="0"+dmonth};
            var ddate=d.getDate();
            if (String(ddate).length==1) {ddate="0"+ddate};
            var ddisplay=dyear+"-"+dmonth+"-"+ddate;
          %>
          <p class="author"><time><%- ddisplay %></time> | <%- documents[i].header.author %></p>
          <hr>
        </article>
      <% }; %>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span12">
      <hr>
      <div class="footer">
        <p><a href="/">&lt;3, Ryan X. Charles</a></p>
      </div>
    </div>
  </div>
</div>
<script src="http://code.jquery.com/jquery.js"></script>
<script src="/bootstrap/js/bootstrap.min.js"></script>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-42842232-1', 'ryanxcharles.com');
  ga('send', 'pageview');

</script>
</body>
</html>
