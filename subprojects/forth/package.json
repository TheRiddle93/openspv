{
  "name": "@coasian/forth",
  "description": "Distributed Forth interpreter.",
  "version": "0.1.0",
  "author": "Coasian Inc.",
  "homepage": "https://github.com/coasian/forth",
  "source": "entry.js",
  "main": "dist/forth.js",
  "module": "dist/forth.modern.js",
  "unpkg": "dist/forth.bundle.js",
  "library": "forth",
  "scripts": {
    "clean": "rm -rf node_modules/* yarn.lock",
    "lint": "standard *.js 'src/**/*.js' 'bin/**/*.js' 'test/**/*.js'",
    "pretest": "yarn run lint",
    "test:mocha": "mocha",
    "test:karma": "karma start --single-run",
    "test": "yarn run test:mocha",
    "build": "webpack && microbundle --target node -f modern,cjs",
    "prepublish": "yarn build",
    "doc": "documentation build entry.js -f html -o doc"
  },
  "keywords": [
    "bitcoin",
    "bsv"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/coasian/forth.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@coasian/spv": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/register": "^7.13.8",
    "@babel/runtime": "^7.13.9",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "documentation": "^13.1.1",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.0",
    "microbundle": "^0.12.0",
    "mocha": "^8.3.1",
    "npm-link-shared": "^0.5.6",
    "should": "^13.2.3",
    "sinon": "^9.2.4",
    "standard": "^16.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "license": "SEE LICENSE",
  "standard": {
    "ignore": [
      "dist/*"
    ]
  }
}
